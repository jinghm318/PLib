&#160;&#160;<span style='color: #000099;'><b>public</b></span>&#160;<span style='color: #000099;'><b>function</b></span>&#160;Unread<span style='color: #0000FF;'>(</span><span style='color: #990000;'><b>$bytes</b></span><span style='color: #0000FF;'>=</span><span style='color: purple;'>1</span><span style='color: #0000FF;'>)</span><br/>
&#160;&#160;<span style='color: #0000FF;'>{</span><br/>
&#160;&#160;&#160;&#160;<span style='color: #990000;'><b>$this</b></span><span style='color: #0000FF;'>-</span><span style='color: #0000FF;'>&#62;</span>cursor&#160;<span style='color: #0000FF;'>-</span><span style='color: #0000FF;'>=</span>&#160;<span style='color: #990000;'><b>$bytes</b></span><span style='color: #0000FF;'>;</span><br/>
