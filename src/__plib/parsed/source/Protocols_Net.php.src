&#160;&#160;<span style='color: #000099;'><b>public</b></span>&#160;<span style='color: #000099;'><b>function</b></span>&#160;<span style='color: #FF7777;'>__construct</span><span style='color: #0000FF;'>(</span>HTTPCookie&#160;<span style='color: #990000;'><b>$cookie</b></span><span style='color: #0000FF;'>=</span>null<span style='color: #0000FF;'>,</span>&#160;<span style='color: #990000;'><b>$encoding</b></span><span style='color: #0000FF;'>=</span><span style='color: #008800;'>&#39;UTF-8&#39;</span><span style='color: #0000FF;'>,</span><br/>
