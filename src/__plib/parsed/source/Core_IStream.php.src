&#160;&#160;<span style='color: #000099;'><b>public</b></span>&#160;<span style='color: #000099;'><b>function</b></span>&#160;Seek<span style='color: #0000FF;'>(</span><span style='color: #990000;'><b>$offset</b></span><span style='color: #0000FF;'>,</span>&#160;<span style='color: #990000;'><b>$whence</b></span><span style='color: #0000FF;'>=</span>SEEK_SET<span style='color: #0000FF;'>)</span><span style='color: #0000FF;'>;</span><br/>
